{
  "name": "minify-kit-dev",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "exports": {
    "demo": "./lib/index.js",
    "import": "./src/index.js"
  },
  "scripts": {
    "gtcfg": "git config --local",
    "alias": "npm run alias:ciam && npm run alias:ada && npm run alias:cisrc && npm run alias:cilib && npm run alias:amlib && npm run alias:mdlib && npm run alias:push",
    "alias:ciam": "npm run gtcfg alias.commit-root 'commit -am'",
    "alias:ada": "cd src/&& git config --local alias.stage-all 'add -A'",
    "alias:cisrc": "npm run gtcfg alias.commit-src '!cd src/&& git stage-all && git commit -m'",
    "alias:cilib": "npm run gtcfg alias.commit-lib '!cd lib/&& git stage-all && git commit'",
    "alias:amlib": "npm run gtcfg alias.amend-lib '!git commit-lib --no-edit --amend'",
    "alias:mdlib": "npm run gtcfg alias.make-lib '!cd src/&& git worktree add ../lib'",
    "alias:push": "npm run gtcfg alias.push-all 'push --all --recurse-submodules=on-demand'",
    "worktree": "git make-lib",
    "commit:src": "git commit-src",
    "commit:lib": "git commit-lib",
    "amend:lib": "git amend-lib",
    "commit": "git commit-root",
    "push": "git push-all",
    "build": "flow-remove-types src --out-dir lib --pretty --extensions .json,.js",
    "demo": "node --conditions=demo demo"
  },
  "keywords": [],
  "author": "Fabrice Sanga (https://github.com/sangafabrice)",
  "license": "ISC",
  "type": "module",
  "workspaces": [ "src" ],
  "devDependencies": {
    "flow-remove-types": "^2.293.0"
  }
}
